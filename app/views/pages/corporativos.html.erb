		<div class="col-md-8 col-md-offset-2" style="padding: 0px;">
			<div id="contact2" class="up">
				<h2>Cont√°ctanos</h2>

			<%= form_for @contact do |f| %>
			<div class="field">
				<%= f.label :Nombre %><br>
				<%= f.text_field :name, required:true %>
			</div>

			<div class="field">
				<%= f.label :Email %><br>
				<%= f.email_field :email, required:true %>
			</div>

			<%= f.text_field :donde, :value => "#{params[:controller]} - #{request.original_url}", class: "hidden" %>

			<div class="field">
				<%= f.label :Mensaje %><br>
				<%= f.text_area :comments, required:true %>
			</div>

			<div class="hidden">
				<%= f.label :nickname %><br>
				<%= f.text_field :nickname, hint: 'Esto se queda en blanco' %>
			</div>

			<div class="but">
				<%= f.submit "Enviar", class:"button button_blue button_center"%>
			</div>
			<% end %>

			</div>
	</div>
